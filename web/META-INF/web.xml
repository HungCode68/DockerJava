<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://jakarta.ee/xml/ns/jakartaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://jakarta.ee/xml/ns/jakartaee
         http://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd"
         version="5.0">

    <servlet>
        <servlet-name>SinhVienServlet</servlet-name>
        <servlet-class>controller.SinhVienServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SinhVienServlet</servlet-name>
        <url-pattern>/sinhvien</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>sinhvien.jsp</welcome-file>
    </welcome-file-list>
</web-app>
